@Article{Canetti2012,
  author       = {Laurent Canetti and Marco Drewes and Mikhail Shaposhnikov},
  title        = {Matter and Antimatter in the Universe},
  abstract     = {We review observational evidence for a matter-antimatter asymmetry in the early universe, which leads to the remnant matter density we observe today. We also discuss observational bounds on the presence of antimatter in the present day universe, including the possibility of a large lepton asymmetry in the cosmic neutrino background. We briefly review the theoretical framework within which baryogenesis, the dynamical generation of a matter-antimatter asymmetry, can occur. As an example, we discuss a testable minimal model that simultaneously explains the baryon asymmetry of the universe, neutrino oscillations and dark matter.},
  date         = {2012-04-18},
  doi          = {10.1088/1367-2630/14/9/095012},
  eprint       = {http://arxiv.org/abs/1204.4186v2},
